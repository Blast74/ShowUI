<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:huddle="http://schemas.huddledmasses.org/wpf/controls"
    xmlns:web="clr-namespace:WebServiceX;assembly=ojtmha83"
    >
   <!-- This Template is for Process, but it only handles processes with Windows!
        And worse, it requires the Huddled.Controls library, and only works on Vista+ -->
    <DataTemplate DataType="{x:Type web:WeatherData}">
        <StackPanel MaxWidth="100" Orientation="Vertical" Margin="2">
            <Image Source='{Binding Path=WeatherImage}' Width="55" Height="58" />
            <StackPanel Orientation="Horizontal" Margin="0"><Label Content="High:"/><Label Content="{Binding Path=MaxTemperatureF}" /></StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0"><Label Content="Low:"/><Label Content="{Binding Path=MinTemperatureF}" /></StackPanel>
            <Label Content="{Binding Path=Day}"/>
        </StackPanel>
    </DataTemplate>
  
</ResourceDictionary>